{
  "cli": {
    "version": ">= 16.18.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.18.0"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "node": "20.18.0",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "node": "20.18.0",
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}